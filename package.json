{
  "name": "rails-partial",
  "displayName": "Rails Partial",
  "description": "Definition and Completion provider for Rails Partial.",
  "version": "0.1.0",
  "publisher": "aki77",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.25.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aki77/vscode-rails-partial.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aki77/vscode-rails-partial/issues"
  },
  "activationEvents": [
    "onLanguage:haml",
    "onLanguage:slim",
    "onLanguage:erb"
  ],
  "main": "./out/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Rails Partial Configuration",
      "properties": {
        "railsPartial.viewFileExtensions": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "html.haml",
            "html.slim",
            "html.erb"
          ],
          "description": "This is the extension of the view files."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/fs-extra": "5.0.2",
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.67",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "fs-extra": "^6.0.1"
  }
}
